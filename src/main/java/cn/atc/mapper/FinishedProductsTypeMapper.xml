<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.atc.mapper.FinishedProductsTypeMapper">
	<insert id="insertFinishedProductsType"
		parameterType="FinishedProductsType">
		insert into
		finishedProductsType(productType,productName)
		values(#{productType},#{productName})
	</insert>
	<select id="getFinishedproductstype"
		resultType="Finishedproductstype">
		select * from Finishedproductstype
	</select>
</mapper>
